#include <iostream>
#include <vector>

class Stack {
private:
    std::vector<int> stack; // Vector to store stack elements

public:
    // Pushes an element onto the stack
    void push(int value) {
        stack.push_back(value);
        std::cout << value << " pushed to stack.\n";
    }

    // Removes the top element from the stack
    void pop() {
        if (stack.empty()) {
            std::cout << "Stack is empty. Cannot pop.\n";
            return;
        }
        int poppedValue = stack.back();
        stack.pop_back();
        std::cout << poppedValue << " popped from stack.\n";
    }

    // Returns the top element of the stack
    int peek() {
        if (stack.empty()) {
            std::cout << "Stack is empty. No top element.\n";
            return -1; // Return -1 if stack is empty
        }
        return stack.back();
    }

    // Checks if the stack is empty
    bool isEmpty() {
        return stack.empty();
    }
};

int main() {
    Stack s;

    s.push(10);
    s.push(20);
    s.push(30);

    std::cout << "Top element is " << s.peek() << "\n";

    s.pop();
    s.pop();
    s.pop();
    s.pop(); // Attempting to pop from an empty stack

    return 0;
}
